Optimized IR:
/*******************************************************
 *                       WARNING                       *
 *  Solidity to Yul compilation is still EXPERIMENTAL  *
 *       It can result in LOSS OF FUNDS or worse       *
 *                !USE AT YOUR OWN RISK!               *
 *******************************************************/

object "C_6" {
    code {
        {
            mstore(64, 128)
            if callvalue() { revert(0, 0) }
            let _1 := datasize("C_6_deployed")
            codecopy(0, dataoffset("C_6_deployed"), _1)
            return(0, _1)
        }
    }
    object "C_6_deployed" {
        code {
            {
                mstore(64, 128)
                revert(0, 0)
            }
        }
    }
}

